{
  "name": "muvistack-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "pnpm install && tsc",
    "start": "cross-env NODE_ENV=production node dist/main.js",
    "dev": "cross-env NODE_ENV=development tsx watch src/main.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "tsc": "tsc",
    "clean": "rimraf dist node_modules && pnpm build",
    "check": "pnpm lint && pnpm tsc",
    "studio": "drizzle-kit studio --config=src/config/drizzle.ts",
    "generate": "drizzle-kit generate:mysql --config=src/config/drizzle.ts",
    "push": "drizzle-kit push:mysql --config=src/config/drizzle.ts"
  },
  "keywords": [],
  "author": "Gia Thinh Nguyen",
  "license": "ISC",
  "dependencies": {
    "@planetscale/database": "^1.13.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.29.2",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mysql2": "^3.6.5",
    "pino": "^8.17.2",
    "dotenv": "^16.3.1",
    "pino-http": "^9.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.9",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.0.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
